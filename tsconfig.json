{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "outDir": "./dist/",
    "module": "es6",
    "target": "es6",
    "strict": true,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowJs": true
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}
