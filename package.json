{
  "name": "search-insights",
  "description": "Library for reporting click, conversion and view metrics using the Algolia Insights API",
  "version": "2.2.1",
  "jsdelivr": "dist/search-insights.browser.umd.js",
  "main": "index-node.cjs.js",
  "types": "index-node.cjs.d.ts",
  "browser": "index-browser.cjs.js",
  "react-native": {
    "http": "./empty-module.cjs.js",
    "https": "./empty-module.cjs.js"
  },
  "engines": {
    "node": ">=8.16.0"
  },
  "files": [
    "dist",
    "empty-module.cjs.js",
    "empty-module.cjs.d.ts",
    "index-browser.cjs.d.ts",
    "index-node.cjs.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf ./dist",
    "build": "yarn build:browser:umd && yarn build:browser:iife && yarn build:browser:cjs && yarn build:node",
    "build:types": "tsc --build tsconfig.declaration.json",
    "build:browser:umd": "vite build --config vite.browser.umd.config.js --emptyOutDir false",
    "build:browser:iife": "vite build --config vite.browser.iife.config.js --emptyOutDir false",
    "build:browser:cjs": "vite build --config vite.browser.cjs.config.js --emptyOutDir false",
    "build:node": "vite build --config vite.node.config.js --emptyOutDir false",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "yarn run lint --fix",
    "test:unit:browser": "vite-jest --config jest.browser.config.js --no-cache",
    "test:unit:node": "vite-jest --config jest.node.config.js --no-cache",
    "test:size": "bundlesize",
    "test:type": "tsc",
    "release": "shipjs prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algolia/search-insights.js.git"
  },
  "author": {
    "name": "Algolia",
    "url": "https://www.algolia.com"
  },
  "license": "MIT",
  "dependencies": {},
  "bundlesize": [
    {
      "path": "./dist/search-insights.umd.js",
      "maxSize": "2.75 kB"
    }
  ],
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@rollup/plugin-replace": "3.0.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.13",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "bundlesize": "0.18.1",
    "eslint": "8.4.1",
    "eslint-config-algolia": "20.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jsdoc": "37.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.5",
    "jest-environment-jsdom": "27.4.4",
    "jest-environment-node": "27.4.4",
    "prettier": "2.5.1",
    "shipjs": "0.24.1",
    "typescript": "4.5.4",
    "vite": "2.7.2",
    "vite-jest": "0.1.4"
  }
}
